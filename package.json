{
  "name": "wdio-jasmine-boilerplate",
  "version": "1.0.0",
  "description": "WebdriverIO with Jasmine and TypeScript",
  "scripts": {
    "compile": "tsc --incremental",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "test": "wdio ./wdio.conf.js",
    "test:headless": "npm run test -- --headless"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgrybyk/wdio-jasmine-boilerplate.git"
  },
  "keywords": [
    "WebdriverIO",
    "wdio",
    "jasmine",
    "typescript",
    "expect-webdriverio"
  ],
  "author": "mykola.grybyk@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mgrybyk/wdio-jasmine-boilerplate/issues"
  },
  "homepage": "https://github.com/mgrybyk/wdio-jasmine-boilerplate#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all --parallel compile lint"
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/jasmine": "^3.6.2",
    "@wdio/cli": "^6.10.2",
    "@wdio/jasmine-framework": "^6.8.0",
    "@wdio/local-runner": "^6.10.2",
    "@wdio/selenium-standalone-service": "^6.10.3",
    "@wdio/spec-reporter": "^6.8.1",
    "@wdio/sync": "^6.10.0",
    "expect-webdriverio": "^1.4.1"
  },
  "bundledDependencies": []
}
